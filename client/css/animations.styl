@keyframes slide-up
  0%
    transform translateY(100%)
    opacity 0.0
  50%
    opacity 1.0
  100%
    transform translateY(0)

@keyframes fade-in
  0%
    transform translateX(20px)
    opacity 0.0
  1%
    transform translateX(16px)
    opacity 0.0
  100%
    opacity 1.0
    transform translateX(0)
